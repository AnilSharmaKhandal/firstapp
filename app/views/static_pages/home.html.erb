    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'profile/user_info' %>
      </section>
      <section class="post_form">
        <%= render 'profile/post_form' %>
      </section>
    </aside>
  </div>
  <div class="col-md-8">
      <h3>Post Feed</h3>
      <%= render 'profile/feed' %>
    </div>
<% else %>
<h1 align="center">Welcome to the First App</h1>
<% end %>

 
 
